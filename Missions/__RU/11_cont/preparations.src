function myisinarea(list,area,x,y);  //test esli jsou uz vsici v te arey, gdyz tak je tam poslu
var tmp,tmp1;
begin
  tmp1=[];
  for tmp in list do if not isinarea(tmp,area) then begin tmp1=tmp1^[tmp];end;
  if tmp1 then begin
    comstop(list diff tmp1);
    commovexy(tmp1,x,y);
    result=false;
  end
  else result=true;
end;

