Function FreeHex(x,y);
     var un;
     begin
          un = HexInfo(x,y);

          if un = 0 then
               begin
                    result = true;
                    exit;
               end;

          if un = -1 then
               begin
                    result = false;
                    exit;
               end;

          if GetType(un) = unit_building or ( GetType(un) = unit_vehicle and ( ( GetControl(un) = control_manual and IsDrivenBy(un) = 0 ) or ( GetControl(un) = control_remote and IsControledBy(un) = 0 ) ) ) then
               result = false
          else
               result = true;
     end;

