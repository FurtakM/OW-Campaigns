// uvodni pokec
EXPORT function uvodni_pokec;
VAR vyber, eng;
begin
  wait(0$2);

  ComTurnUnit(bur,kur);                                       
  ComTurnUnit(kur,bur);
  if BELKOV_SAVED
    then
      begin
        Say(kur,'D1b-Kur-1');
        Say(bel,'D1b-Bel-1');
        Say(kur,'D1b-Kur-2');
      end
    else
      begin
        Say(kur,'D1a-Kur-1');
        Say(bur,'D1a-Bur-1');
        if STAV_UTOKU=3 then Say(kur,'D1a-Kur-2a')
                        else Say(kur,'D1a-Kur-2');
        Say(kur,'D1a-Kur-2b');
        Say(bur,'D1a-Bur-2');
      end;
  Say(kur,'D2-Kur-1');
  Say(bur,'D2-Bur-1');
  Say(kur,'D2-Kur-2');
  Say(bur,'D2-Bur-2');
  Say(kur,'D2-Kur-3');
  Say(pop,'D2-Pop-3');
  Say(kur,'D2-Kur-4');
  Say(bur,'D2-Bur-4');
  Say(kur,'D2-Kur-5');
  Say(bur,'D2-Bur-5');
  Say(kur,'D2-Kur-6');
  vyber=vyber_characteru;
  if bel in vyber then RemoveUnit(bel)
                  else Setside(bel,You2);
  if IsOK(kov) then RemoveUnit(kov);
  if IsOK(gny) then RemoveUnit(gny);
  RemoveUnit(bur);
  prebarveni_druhe_baze;
  ChangeSideFog(You3,You);
  placni_unity(vyber);
  CenterOnUnits(vyber);
  RevealFogArea(You,Wth);
  wait(0$2);
  ChangeMissionObjectives('M1');
  hint_bw=true;
  Hint('Oil');
  Hint('Upgrading');
  InGameOff;

  wait(1$00);

  eng := FilterAllUnits([[f_side, 6], [f_class, 2]]);

  if not eng then
     exit;

  ComExitBuilding(eng);
  AddComBuild(eng, b_oil_power, 140, 69, 0);

  wait(2$30);

  ComEnterUnit(eng, RU1_dep);
end;              

