EVERY 0$5 TRIGGER FilterAllUnits([[f_side,You],[f_btype,b_depot]])>1 DO
VAR pom,pom1;
VAR i;
VAR pocet;
begin
  pocet=0;
  pom=FilterAllUnits([[f_side,You],[f_btype,b_depot]]);
  pom1=pom;
  while pocet<2 do
    begin
      for i in pom do
        if not IsConstructed(i) then begin
                                       pocet=pocet+1;
                                       pom1=pom1 diff i;
                                     end;
      pom=pom1;
      wait(0$2);
    end;
//  Hint('Transport');
  Hint('Transport1');
end;

