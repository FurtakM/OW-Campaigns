        a:=WhoSayAny(not_char, you, 0, class_mechanic, 0);
        TalkOn;
        if a then
          if not SayAny(a, 'D3a-Mech1-1', you, sex_male, class_mechanic, 0) then
                 SayAny(a, 'D3a-FMech1-1', you, sex_female, class_mechanic, 0);
