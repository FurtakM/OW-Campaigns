           while mytick>tick and GetDistUnits(Joan, JMM)>5 do
                 Wait(0$1);
//           InGameOff;
           DialogueOn;
           if IsLive(Joan) and IsLive(JMM) then begin
              Say(Joan, 'D5a-Joan-4');     
